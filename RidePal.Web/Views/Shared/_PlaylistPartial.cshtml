@model PlaylistVM

<div>
    <form id="playlist-save-form" method="POST" asp-action="SavePlaylist" asp-controller="Playlists">
        @for (int i = 0; i < Model.TrackPlaylists.Count; i++)
        {
            var item = Model.TrackPlaylists.ElementAt(i);
            @Html.HiddenFor(m => m.TrackPlaylists.ElementAt(i).TrackId)
            @Html.Partial("_TrackPartial", item.Track)
        }
        <button type="submit">Submit</button>
    </form>
</div>